#include<iostream>
void shiftArray(int* M, int n, int k) {
    if (k > n) {
        k = k % n;
    }
    for (int i = n - k; i < n; i++){
        std::cout << M[i] << " ";
    }
    for (int i = 0; i < n - k; i++){
        std::cout << M[i] << " ";
    } 
}

int main() {
    setlocale(LC_ALL, "ru");
    std::cout << "Введите длину массива и необходимый сдвиг: " << std::endl;
    int n, shift;
    std::cin >> n >> shift;

    int* arr = new int[n];
    std::cout << "Введите элементы массива" << std::endl;
    for (int i = 0; i < n; ++i) {
        std::cin >> arr[i];
    }
    shiftArray(arr, n, shift);
    delete[] arr;
    return 0;
}
