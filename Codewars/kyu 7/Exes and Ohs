bool XO(const std::string& str)
{
  int amount_of_x = 0, amount_of_o = 0;
  for(int i = 0; i < str.length(); ++i){
    if (str[i] == 'x' or str[i] == 'X'){
      ++amount_of_x;
    }
    else if (str[i] == 'o' or str[i] == 'O'){
      ++amount_of_o;
    }
  }
  return (amount_of_o == amount_of_x);
}
